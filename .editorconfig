# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space


[*.{js,jsx}]
indent_size = 4

[*.pcss]
indent_size = 4

[{package.json}]
indent_size = 2
